.cpp.obj:
	$(CXX) -c $(INCDIRS) $(CXXFLAGS) $(DEFINES) $<

all: $(FOXLIB)

FXApp.cpp: icons.cpp

icons.h: $(ICONS)
	$(RESWRAP) -h -n FX -o icons.h $(ICONS)

icons.cpp: $(ICONS) icons.h
	$(RESWRAP) -i icons.h -n FX -oa icons.cpp $(ICONS)

clean:
	del $(FOXLIB)
	del *.obj

# Make the library
$(FOXLIB): $(OBJECTS)
	touch $(FOXLIB)
	del $(FOXLIB)
	$(RCC) -32 -oversion.res version.rc
	$(LIB) -c -p64 $(FOXLIB) $(OBJECTS)
